{
  "expo": {
    "name": "varify",
    "slug": "varify",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "synex-varify",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "19",
      "supportsTablet": true,
      "bundleIdentifier": "com.synex.varify",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": "12",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.synex.varify"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7b3fa9dc-6e0b-4945-99f5-6564fae037c8"
      }
    },
    "owner": "kyemoon"
  }
}
